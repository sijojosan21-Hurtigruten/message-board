{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "message-board-ui",
      "image": "sijojosan/message-board-ui",
      "hostname": "message-board-ui",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "sijojosan/message-board-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["message-board-ui"],
      "memory": 128
    }
  ]
}
